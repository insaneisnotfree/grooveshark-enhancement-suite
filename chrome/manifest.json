{
      "name": "Grooveshark Enhancement Suite"
    , "description": "A suite of handy modules that improve the Grooveshark experience"
    , "version": "1.1"
    , "background_page": "background.html"
    , "permissions": ["http://lyrics.wikia.com/*", "http://*.grooveshark.com/*"]
    , "content_scripts": [
        {
              "matches": ["http://*.grooveshark.com/*"]
            , "js": ["jquery.min.js", "contentscript.js"]
        }
    ]
    , "icons":
        {
              "48": "icons/48.png"
            , "128": "icons/128.png"
        }
}
